	Keywords = {diffusion equations, locally conservative method, mimetic discretizations},
	Month = {September},
	Number = {3-4},
	Pages = {333-325},
	Title = {Mimetic Finite Difference Methods for Diffusion Equations},
	Volume = {6},
	Year = {2002},
